<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#ff7f00" />
            <stroke
                android:width="1dp" />
            <corners
                android:radius="12dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#583e20" />
            <stroke
                android:width="1dp" />
            <corners
                android:radius="12dp" />
        </shape>
    </item>
</selector>